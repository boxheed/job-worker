{
  "name": "nats-job-worker",
  "version": "0.2.0",
  "description": "CLI package for NATS JetStream job worker",
  "type": "module",
  "main": "src/index.js",
  "bin": {
    "nats-fs-worker": "./bin/worker.js"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "preferGlobal": true,
  "scripts": {
    "test": "vitest run --sequence.concurrent=false",
    "start": "node bin/worker.js",
    "lint": "eslint 'src/**/*.js' 'tests/**/*.js'",
    "format": "prettier --write 'src/**/*.js' 'tests/**/*.js'",
    "release": "standard-version --commit-all"
  },
  "keywords": [
    "nats",
    "jetstream",
    "worker",
    "job-executor",
    "cli"
  ],
  "author": "Andrew Dunn",
  "license": "Apache-2.0",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "eslint": "^10.0.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "globals": "^16.0.0",
    "prettier": "^3.8.1",
    "standard-version": "^9.5.0",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "commander": "^14.0.3",
    "nats": "^2.29.3"
  }
}
